#run this after running compileallfixed
./changeformat changeformat1.cbl changeformat2.cbl tofree
rm changeformat2
cobc -x changeformat2.cbl
chmod +x changeformat2

./changeformat errnomessage1.cbl errnomessage2.cbl tofree
./changeformat httpstatus1.cbl httpstatus2.cbl tofree
./changeformat connecttoserver1.cbl connecttoserver2.cbl tofree
./changeformat gettimestamp1.cbl gettimestamp2.cbl tofree

./changeformat tcpipserver1.cbl tcpipserver2.cbl tofree
rm tcpipserver2
cobc -x -W tcpipserver2.cbl \
  get_errno.c \
  errnomessage2.cbl \
  httpstatus2.cbl \
  connecttoserver2.cbl \
  gettimestamp2.cbl
chmod +x tcpipserver2


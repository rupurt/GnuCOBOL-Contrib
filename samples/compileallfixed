rm changeformat
cobc -x changeformat.cbl
chmod +x changeformat
./changeformat changeformat.cbl changeformat1.cbl tofixed
rm changeformat1
cobc -x changeformat1.cbl
chmod +x changeformat1

./changeformat errnomessage.cbl errnomessage1.cbl tofixed
./changeformat httpstatus.cbl httpstatus1.cbl tofixed
./changeformat connecttoserver.cbl connecttoserver1.cbl tofixed
./changeformat gettimestamp.cbl gettimestamp1.cbl tofixed

./changeformat tcpipserver.cbl tcpipserver1.cbl tofixed
rm tcpipserver1
cobc -x -W tcpipserver1.cbl \
  get_errno.c \
  errnomessage1.cbl \
  httpstatus1.cbl \
  connecttoserver1.cbl \
  gettimestamp1.cbl
chmod +x tcpipserver1

